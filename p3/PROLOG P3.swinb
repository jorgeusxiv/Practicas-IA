%%%Concatena%%%

concatena([],L,L).
concatena([X|L1], L2,[X|L3]):- 
    concatena(L1,L2,L3).

%%%Invierte%%%

invierte([],[]).
invierte([H|T],ListaInv):-
    invierte(T,InvT), concatena(InvT, [H], ListaInv).

