%%%Ejercicio 4%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

divide(L,N, L1, L2) :-
   length(L1, N),
   append(L1, L2, L).

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%Ejercicio 5 Preguntar JESSICA%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

aplasta([], []).
aplasta([L|Ls], L1) :-
    aplasta(L, First),
   	aplasta(Ls, Rest),
    append(First, Rest, L1).
aplasta(L, [L]).

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%Ejercicio 6%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

primos(N,L):- findall(D, primo(N,D),L).

primo(N,D):-
    buscar_primo(N,2,D).

buscar_primo(N,D,D):-
    0 is N mod D.
buscar_primo(N,D,R):-
    D &lt; N,
    (0 is N mod D
    -&gt;  (N1 is N/D, buscar_primo(N1,D,R))
    ;   (D - 2 &gt; 0
        -&gt;  (D1 is D + 2, buscar_primo(N, D1, R))
        ;   (D2 is D + 1, buscar_primo(N, D2, R))
    )).

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%Ejercicio 7%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%7.1%%%

pack([], []).
pack([X], [[X]]).
pack([X, X| L], [[X| Xs]| R]) :-
    pack([X| L], [Xs| R]).
pack([X, Y| L], [[X]| R]) :-
    X \= Y,
    pack([Y| L], R).


    
    
    


